auth           on
tls            on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile        ~/.msmtp.log

account        gmail
host           smtp.gmail.com
port           587
from           dwrz@dwrz.net
user           dwrz@dwrz.net
passwordeval   pass google/gmail-app-pw

# Set a default account
account default : gmail
